arameters":["\\n<哈迪斯>哎？"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<帝俊>emm虽然不太好意思麻烦你但是，"]},{"code":401,"indent":0,"parameters":["事实上刚刚逃命的时候我的刀不知道掉在哪里了，"]},{"code":401,"indent":0,"parameters":["我想如果再碰到怪物的话，果然武器还是必要的。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<帝俊>如果可以的话，你能帮我找一下吗？"]},{"code":101,"indent":0,"parameters":["hds",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<哈迪斯>（……你是怎么做到的捡了钥匙丢了刀啊？）"]},{"code":101,"indent":0,"parameters":["hds",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<哈迪斯>啊那个啊，太巧了，其实就在隔壁哎，"]},{"code":401,"indent":0,"parameters":["我已经找到了，还给你。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<帝俊>啊哈哈……是吗？呃，谢，谢谢……"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["将【三途河】还给了帝俊！"]},{"code":127,"indent":0,"parameters":[3,1,0,1,false]},{"code":101,"indent":0,"parameters":["hds",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<哈迪斯>不用谢，那么就去四楼吧？"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<帝俊>不，等，等等！"]},{"code":401,"indent":0,"parameters":["话说我刚才跑了半天也有点饿了，"]},{"code":401,"indent":0,"parameters":["你身上有带吃的吗？"]},{"code":101,"indent":0,"parameters":["hds",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<哈迪斯>啊，那咱们可以先去一楼的书房，"]},{"code":401,"indent":0,"parameters":["我记得那里有（可以回一万滴血的）面包，"]},{"code":401,"indent":0,"parameters":["你可以吃一点。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<帝俊>咱、咱们老钟人，吃不惯西式面包，"]},{"code":401,"indent":0,"parameters":["哈！哈！哈哈哈……"]},{"code":101,"indent":0,"parameters":["hds-nervous",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<哈迪斯>（……这人到底什么毛病？）"]},{"code":101,"indent":0,"parameters":["hds-nervous",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<哈迪斯>你这种症状就是玩原神玩的，"]},{"code":401,"indent":0,"parameters":["这种地方你还指望吃上满汉全席吗？"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<帝俊>拜托了除了面包什么都可以！"]},{"code":401,"indent":0,"parameters":["这样饿肚子我实在没有力气啊，"]},{"code":401,"indent":0,"parameters":["不想想别的办法吗？"]},{"code":101,"indent":0,"parameters":["hds",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<哈迪斯>（……他不会真的在里面玩原神吧？）"]},{"code":101,"indent":0,"parameters":["hds",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<哈迪斯>算了，拿你没办法，"]},{"code":401,"indent":0,"parameters":["我再出去找一圈吧，"]},{"code":401,"indent":0,"parameters":["反正还得找找阿芙洛狄忒去哪了……"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<帝俊>是吗！太好了！得救了！"]},{"code":401,"indent":0,"parameters":["那顺带一提，你找到后土了吗？"]},{"code":101,"indent":0,"parameters":["hds",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<哈迪斯>还没有……"]},{"code":401,"indent":0,"parameters":["不过总会有办法的。"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<帝俊>那多事抱歉了。"]},{"code":401,"indent":0,"parameters":["作为感谢，你顺便收下这个吧。"]},{"code":126,"indent":0,"parameters":[10,0,0,1]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["获得了【咖啡】！"]},{"code":101,"indent":0,"parameters":["hds",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<哈迪斯>（饮料？而且还是咖啡？）"]},{"code":101,"indent":0,"parameters":["hds",0,0,2]},{"code":401,"indent":0,"parameters":["\\n<哈迪斯>（门里到底是什么地方啊……）"]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["【零元购商店】已经开启了哦！"]},{"code":401,"indent":0,"parameters":["在一楼某个神奇的地方找找看吧！"]},{"code":121,"indent":0,"parameters":[19,19,0]},{"code":123,"indent":0,"parameters":["B",0]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true},{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"B","selfSwitchValid":true,"sw